{
  "singleQuote": true,
  "printWidth": 120,
  "tabWidth": 4,
  "useTabs": false,
  "semi": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "overrides": [
    {
      "files": "{apps,libs}/**/*.html",
      "options": {
        "parser": "angular"
      }
    }
  ]
}
